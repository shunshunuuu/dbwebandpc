							'<img src="images/pronews_bg001.jpg" alt="" />'+
						'</div>'+
						'<div class="sibnews_r">'+
							'<div class="newstits">'+
								'<p>'+product_name+'</p>'+
								'<span>订阅9999</span>'+
							'</div>'+
							'<div class="thestarts">推荐指数：'+
//						 从一颗星到5颗星，分别添加onestart,twostart,threestart,fourstart,fivestart;startbox为默认； 
								'<a class="startbox "+"'+classes+'" href="javascript:;">' +
									'<span class="str01"></span>' +
									'<span class="str02"></span>' +
									'<span class="str03"></span>' +
									'<span class="str04"></span>' +
									'<span class="str05"></span>' +
								'</a>' +
							'</div>' +
							'<div class="theprice"><span>10东北币</span></div>'+
							'<div class="exchange">' +
								'<a href="javascript:;" id="exchange">立即兑换</a>' +
							'</div>' +
						'</div>' +
					'</div>' +
				'</li>';				

					  zxProductEle.append(itemHtml);
					}
			    }else{
			    	zxProductELe.append('<li style="text-align:center; color:#c0c2c6; ">暂无数据</li>');
			    }
			}else{
				layerUtils.iAlert("查询资讯产品列表失败:"+resultVo.error_info,-1);
			}
		})
		
	}
	/*
	 * 绑定事件
	 */
	function bindPageEvent() {
		// 返回
		appUtils.bindEvent(_pageId + ".back_btn", function() {
			appUtils.pageBack();
		});
		
		//搜索
		appUtils.bindEvent(_pageId + ".per_search", function(){
			appUtils.pageInit(pageCode,"infoproduct/searchInfo");
		});
		
		//立即兑换
		appUtils.bindEvent(_pageId + "#exchange", function(){
			var productid = $(this).parent().parent(); 
//			appUtils.pageInit(pageCode, "infoproduct/infoDetails",{"productid":productid});
			appUtils.pageInit(pageCode, "infoproduct/infoDetails");
		});
		
		appUtils.bindEvent(_pageId + "#infoproduct", function(){
			var index = $(this).index(this);
			var productid = $("#infoproduct li").attr("productid");
			for (var i = 0; i <= index.length; i++) {
				switch(i){
		          case i:
		    	    appUtils.pageInit(pageCode,"infoproduct/infoDetails",{"productid":productid});
		    	    break;
		        }
			}
			
		})
	};
	


	/*
	 * 页面销毁
	 */
	function destroy() {};

	var main = {
		"init" : init,
		"bindPageEvent" : bindPageEvent,
		"destroy" : destroy
	};
	
	// 暴露对外的接口
	module.exports = main;
});