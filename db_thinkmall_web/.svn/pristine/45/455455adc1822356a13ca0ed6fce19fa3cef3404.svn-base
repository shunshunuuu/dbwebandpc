		<!--弹窗-->
		<div class="window_dialog" id="order_buy" style="display: none;">
			<div class="dialog_cet">
				<a href="javascript:void(0);" class="close_btn" id="close_btn_buy"></a>
				<div class="dialog_tit2">
					<h3>请输入交易密码</h3>
				</div>
				<div class="dialog_cet_det">
					<div class="dialog_text">
						<p>确认购买</p>
						<strong id="top_price_value">¥0.00</strong>
						<div class="pass_text" id="trade_pwd_box">
							<input type="text" readonly unselectable="on" id="trade_pwd"
								class="text" readonly="readonly" keyboard-plugin="true">
							<em></em><em></em><em></em><em></em><em></em><em></em>
						</div>
					</div>
				</div>
			</div>
		</div>
		<!--弹窗1-->
		<div class="window_dialog" id="window_one" style="display: none;">
