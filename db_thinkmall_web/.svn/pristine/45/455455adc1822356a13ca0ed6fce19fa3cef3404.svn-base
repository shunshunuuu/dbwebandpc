				</div>
				<div class="agreemet_box" style="display: none;">
					<div class="ui checkbox">
						<input type="checkbox" id="checkbox_2" checked="checked" /><label></label><a
							id="agreement" href="javascript:void(0)">《基金户开通协议》</a>
					</div>
				</div>
				<div class="ui layout btn_box">
					<div class="row-1">
						<a href="javascript:void(0)" class="save_btn" id="cancelFund"
							style="width: 50%; float: left;">取消</a> <a
							href="javascript:void(0)" class="save_btn" id="openFund"
							style="width: 50%; float: left;">开通</a>
					</div>
				</div>
			</div>
		</div>

		<!--弹窗-->
		<div class="window_dialog" id="window_three" style="display: none;">
			<div class="dialog_cet dialog_cet3">
				<div class="order_det_list">
					<div class="order_det_tit">
						<h3>客户信息确认</h3>
					</div>
					<div class="order_det_table">
						<table cellpadding="0" cellspacing="0" border="0" width="100%">
							<tr>
								<th>用户姓名:</th>
								<td id="windowUserName"></td>
							</tr>
							<tr>
								<th>证件号码:</th>
								<td id="windowIDNo"></td>
							</tr>
							<tr>
								<th>联系电话:</th>
								<td id="windowPhone"></td>
							</tr>
							<tr>
								<th>详细地址:</th>
