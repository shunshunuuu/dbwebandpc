				<div class="login_list_det">
					<div class="text_item">
						<div class="ui field border rounded">
							<input type="password" id="tradePwd" class="ui input" placeholder="请输入6位数字交易密码" readonly="readonly">
							<a href="javascript:void(0)" class="del_btn"></a>
						</div>
					</div>
					<div class="text_item">
						<div class="ui field border rounded">
							<input type="password" id="tradePwdTwice" class="ui input" placeholder="请再次输入密码" readonly="readonly">
							<a href="javascript:void(0)" class="del_btn"></a>
						</div>
					</div>
				</div>
				<h4 class="list_tit">同时开通账户</h4>
				<div class="login_list_det">
					<a href="javascript:void(0)" class="default_check default_act">OTC</a>
