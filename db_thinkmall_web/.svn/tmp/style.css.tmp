}
.ui.button.warning {
	background: rgb(223, 117, 20);
}
.ui.button.secondary {
	background: rgb(66, 184, 221);
}

.ui.buttons {
	margin:0 -0.05rem;
	display:-webkit-box;
	display:-webkit-flex;
	display:flex;
}
.ui.buttons > .ui.button {
	width:100%;
	margin:0 0.05rem;
}
.ui.button-group {
	display:-webkit-box;
	display:-webkit-flex;
	display:flex;
}
.ui.button-group > .ui.button {
	width:100%;
