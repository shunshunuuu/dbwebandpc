/** Body, links, basics **/
body,html{
	width: 100%;
	height: 100%;
	font-size: 100px;
	overflow-x: hidden;
	-webkit-overflow-scrolling:touch;
}
body {
	font-size:0.14rem;
	line-height: 1.8;
	font-family: STHeiti STXihei,Microsoft JhengHei,Microsoft YaHei,Arial;
	-webkit-font-smoothing: antialiased;
