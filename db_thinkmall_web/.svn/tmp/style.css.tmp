.highlight{
	position: relative;
	z-index: 10000;
}
.highlight:before{
	content: "";
	width: 0.6rem;
	height: 0.6rem;
	background:#FFFFFF;
	border-radius: 100%;
	-webkit-border-radius: 100%;
	-moz-border-radius: 100%;
	position: absolute;
	bottom: 0;
	left: 50%;
	margin-left:-0.3rem; 
	z-index: -1;
	/*-webkit-box-shadow:inset 0 0 10px #fff; */
	-webkit-box-shadow: 0 0 5px rgba(255, 255, 255, 0.75);
}
.highlight:after{
	content: "";
	width: 0.6rem;
	height:0.6rem;
	background:#FFFFFF;
	border-radius: 100%;
	-webkit-border-radius: 100%;
	-moz-border-radius: 100%;
	position: absolute;
	bottom: 0;
	left: 50%;
	margin-left: -0.3rem; 
	z-index: -1;
	-webkit-animation: btnzoom 2s ease infinite ;
	-moz-animation: btnzoom 2s ease infinite ;
	animation: btnzoom 2s ease infinite;
}

@-moz-keyframes btnzoom {
	0% {
		opacity:1;
		-webkit-transform: scale3d(1,1,.3);
		-moz-transform: scale3d(1,1,.3);
		transform: scale3d(1,1,.3);
	}
	100% {
		opacity:0;
		-webkit-transform: scale3d(1.3,1.3,.3);
		-moz-transform: scale3d(1.3,1.3,.3);
		transform: scale3d(1.3,1.3,.3);
	}
}
@-webkit-keyframes btnzoom {
	0% {
		opacity:1;
		-webkit-transform: scale3d(1,1,.3);
		-moz-transform: scale3d(1,1,.3);
		transform: scale3d(1,1,.3);
	}
	100% {
		opacity:0;
		-webkit-transform: scale3d(1.3,1.3,.3);
		-moz-transform: scale3d(1.3,1.3,.3);
		transform: scale3d(1.3,1.3,.3);
	}
}
@keyframes btnzoom {
	0% {
		opacity:1;
		-webkit-transform: scale3d(1,1,.3);
		-moz-transform: scale3d(1,1,.3);
		transform: scale3d(1,1,.3);
	}
	100% {
		opacity:0;
		-webkit-transform: scale3d(1.3,1.3,.3);
		-moz-transform: scale3d(1.3,1.3,.3);
		transform: scale3d(1.3,1.3,.3);
	}
}

